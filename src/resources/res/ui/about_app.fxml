<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="hBoxFather" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sepisoad.bongah.ui.view.AboutAppController">
   <children>
      <VBox alignment="TOP_CENTER">
         <children>
            <Label id="idAppName" fx:id="labelSoftwareName" />
            <Label fx:id="labelSoftwareVersion" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </padding>
      </VBox>
      <GridPane hgap="40.0" vgap="10.0">
        <columnConstraints>
          <ColumnConstraints hgrow="NEVER" />
          <ColumnConstraints hgrow="SOMETIMES" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="lebelDeveloperTitle" />
            <Label fx:id="lebelEmailTitle" GridPane.rowIndex="1" />
            <Label fx:id="lebelEmailValue" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="lebelDeveloperValue" GridPane.columnIndex="1" />
            <Label fx:id="lebelPhoneTitle" GridPane.rowIndex="2" />
            <Label fx:id="lebelPhoneValue" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
   <padding>
      <Insets bottom="50.0" left="50.0" right="50.0" top="10.0" />
   </padding>
</VBox>
