<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="hBoxFather" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sepisoad.bongah.ui.view.AboutOwnerController">
   <children>
      <VBox alignment="TOP_CENTER">
         <children>
            <Label fx:id="labelSoftwareName" />
            <Label fx:id="labelSoftwareVersion" />
         </children>
         <padding>
            <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
         </padding>
      </VBox>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="lebelDeveloperTitle" />
            <Label fx:id="lebelEmailTitle" GridPane.rowIndex="1" />
            <Label fx:id="lebelEmailValue" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="lebelDeveloperValue" GridPane.columnIndex="1" />
            <Label fx:id="lebelPhoneTitle" GridPane.rowIndex="2" />
            <Label fx:id="lebelPhoneValue" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</VBox>
