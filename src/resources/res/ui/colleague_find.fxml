<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sepisoad.bongah.ui.view.ColleagueFindController">
   <children>
      <HBox spacing="10.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Label fx:id="labelName" HBox.hgrow="ALWAYS" />
            <TextField fx:id="textFieldName" HBox.hgrow="ALWAYS" />
            <Label fx:id="labelPhoneNumbers" HBox.hgrow="ALWAYS" />
            <TextField fx:id="textFieldPhoneNumbers" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <ScrollPane fx:id="scrollPaneResult" fitToHeight="true" fitToWidth="true" VBox.vgrow="ALWAYS" />
      <HBox spacing="10.0" VBox.vgrow="NEVER">
         <children>
            <Button fx:id="buttonFind" mnemonicParsing="false" onAction="#onButtonFindAction" HBox.hgrow="NEVER" />
            <Button fx:id="buttonUpdate" mnemonicParsing="false" onAction="#onButtonUpdateAction" />
            <Button fx:id="buttonSelect" mnemonicParsing="false" onAction="#onButtonSelectAction" />
            <Separator visible="false" HBox.hgrow="ALWAYS" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding></HBox>
   </children>
</VBox>
