<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sepisoad.bongah.ui.view.CustomerNewController">
   <children>
      <GridPane hgap="10.0" vgap="10.0">
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="NEVER" />
          <ColumnConstraints halignment="CENTER" hgrow="NEVER" minWidth="300.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="labelName" />
            <TextField fx:id="textFieldName" GridPane.columnIndex="1" />
            <Label fx:id="labelPhoneNumbers" GridPane.rowIndex="1" />
            <TextField fx:id="textFieldPhoneNumbers" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="labelAddress" GridPane.rowIndex="2" />
            <TextField fx:id="textFieldAddress" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </GridPane>
      <Pane VBox.vgrow="ALWAYS" />
      <HBox spacing="10.0" VBox.vgrow="NEVER">
         <children>
            <Button fx:id="buttonSave" mnemonicParsing="false" onAction="#onButtonSaveAction" HBox.hgrow="NEVER" />
            <Separator visible="false" HBox.hgrow="ALWAYS" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding></HBox>
   </children>
</VBox>
